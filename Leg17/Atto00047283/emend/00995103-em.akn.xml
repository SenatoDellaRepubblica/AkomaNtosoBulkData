			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#IDD3ZUJ0HXHQAIFSXYOIZMQBVEELHHELSRDDM5QDM0EZ5QMJUKMPCC">CANDIANI</an:docProponent>
