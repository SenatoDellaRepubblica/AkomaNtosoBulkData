			<an:p>
				<an:docProponent refersTo="#IDG0S1VOPCMWREIRI5FK00DRXYSPQ3XDFOYZ0Y2DE5HSUGKUJT3YSK" as="#tipoSenatore">CANDIANI</an:docProponent>
