		<an:amendmentBody>
			<an:amendmentContent>
				<an:p>
					<an:i>All'articolo 3, comma 2, sostituire le parole:</an:i>
					 &quot;di cui al comma 1&quot; 
					<an:i>con la seguente:</an:i>
					 &quot;promotore&quot; 
					<an:i>e le parole:</an:i>
					 &quot;delle risorse finanziarie&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;del contributo straordinario&quot;;
				</an:p>
				<an:p>
					<an:i>All'articolo 3, comma 4, sostituire le parole:</an:i>
					 &quot;dell'arte letteraria&quot; 
					<an:i>con la seguente:</an:i>
					 &quot;letteratura&quot; 
					<an:i>e le parole:</an:i>
					 &quot;e tra essi&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;tra i quali&quot;;
				</an:p>
				<an:p>
					<an:i>All'articolo 3, comma 5, sostituire le parole:</an:i>
					 &quot;delle specifiche&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;di ogni specifica&quot;;
				</an:p>
				<an:p>
					<an:i>All'articolo 3, comma 6, dopo le parole:</an:i>
					 &quot;a carico del contributo&quot;, 
					<an:i>inserire la seguente:</an:i>
					 &quot;straordinario&quot;;
				</an:p>
				<an:p>
					<an:i>All'articolo 3, comma 7, sostituire le parole:</an:i>
					 &quot;bando emanato&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;bando da emanare&quot;;
				</an:p>
				<an:p/>
			</an:amendmentContent>
		</an:amendmentBody>
