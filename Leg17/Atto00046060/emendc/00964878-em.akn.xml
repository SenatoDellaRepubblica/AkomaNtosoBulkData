			<an:p>
				<an:docProponent refersTo="#IDQNHKJFZJ2O4YC4G02P2S3VXMXNMWF2X2QVIFDSB2MJJGTUWNEKZ" as="#tipoSenatore">Bignami, Simeoni, Vacciano, Molinari, Maurizio Romani, Gambaro</an:docProponent>
