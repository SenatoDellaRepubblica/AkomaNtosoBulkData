				d’iniziativa dei deputati 
				<an:docProponent refersTo="#w1ab1ab1b2aaab1" as="#w1ab1ab1b2aaab1a">SERENI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaab3" as="#w1ab1ab1b2aaab1a">ASCANI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaab5" as="#w1ab1ab1b2aaab1a">CIPRINI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaab7" as="#w1ab1ab1b2aaab1a">GALGANO</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaab9" as="#w1ab1ab1b2aaab1a">GALLINELLA</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaac11" as="#w1ab1ab1b2aaab1a">GIULIETTI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1ab1ab1b2aaac13" as="#w1ab1ab1b2aaab1a">LAFFRANCO</an:docProponent>
				 e 
