			<an:article id="d2474e138">
				<an:num>1</an:num>
				<an:paragraph id="d2474e140">
					<an:num>1</an:num>
					<an:list id="d2474e141">
						<an:intro id="d2474e142">
							<an:p>
								All'articolo 609-
								<an:i>septies</an:i>
								 del codice penale sono apportate le seguenti modificazioni:
							</an:p>
						</an:intro>
						<an:point id="d2474e147">
							<an:num>a</an:num>
							<an:content id="d2474e149">
								<an:p>
									al primo comma, le parole: «articoli 609-
									<an:i>bis</an:i>
									, 609-
									<an:i>ter</an:i>
									 e 609-
									<an:i>quater</an:i>
									» sono sostituite dalle seguenti: «articoli 609-
									<an:i>bis</an:i>
									 e 609-
									<an:i>ter</an:i>
									»;
								</an:p>
							</an:content>
						</an:point>
						<an:point id="d2474e167">
							<an:num>b</an:num>
							<an:content id="d2474e169">
								<an:p>al quarto comma, il numero 5) è abrogato.</an:p>
							</an:content>
						</an:point>
					</an:list>
				</an:paragraph>
			</an:article>
		</an:body>
