					<an:references source="#redattore">
						<an:original href="http://dati.senato.it/osr/Ddl/2015-12-29/2184/ita@/main/relazione" id="ro2" showAs="Relazione"/>
						<an:attachmentOf href="http://dati.senato.it/osr/Ddl/2015-12-29/2184/ita@/main" id="mainAnx2" showAs="Main"/>
					</an:references>
