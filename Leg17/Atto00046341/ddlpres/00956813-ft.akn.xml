					<an:references source="#redattore">
						<an:original id="ro2" href="http://dati.senato.it/osr/Ddl/2015-12-29/2184/ita@/main/relazione" showAs="Relazione"/>
						<an:attachmentOf id="mainAnx2" href="http://dati.senato.it/osr/Ddl/2015-12-29/2184/ita@/main" showAs="Main"/>
