		<an:preface>
			<an:p>
				<an:affectedDocument href="#ar1">DDL 2134</an:affectedDocument>
			</an:p>
			<an:p>
				<an:docNumber>Emendamento n. 1.17</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#IDW22G3UISXBIQJMQCGG3POJXRBKZH3PI42JFQJEE3NWWLBDA3DKGK" as="#tipoSenatore">D'ASCOLA</an:docProponent>
