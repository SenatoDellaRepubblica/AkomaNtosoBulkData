			<an:p>
				<an:docProponent refersTo="#ID5PZ4JYI1VP2LO1ZDQHLY35ZF0CP14QYK5ESA31NORE5RPNXPGAZ" as="#tipoSenatore">DI GIORGI</an:docProponent>
