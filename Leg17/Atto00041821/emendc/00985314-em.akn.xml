			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID4NAULWZW2H4JGOAMBJFXCQJ10BK1UBPBUHZKY4O0QAJPHGZ0P0GO" as="#tipoSenatore">BIANCONI</an:docProponent>
