				<an:docNumber>Emendamento n. 9.200/12</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID53S4BWNOG0YIDFG3AROHNJNXCDMZWM2PPOFNGWJETQBXOOJ0X2RM" as="#tipoSenatore">ZIZZA</an:docProponent>
