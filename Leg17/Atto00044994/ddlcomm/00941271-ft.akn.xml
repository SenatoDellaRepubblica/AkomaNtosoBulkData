						<an:docProponent refersTo="#" as="#d85e73a1327">Ministro dello sviluppo economico ()</an:docProponent>
						con il
                                        
						<an:docProponent refersTo="#" as="#d85e75a1327">Ministro delle infrastrutture e dei trasporti ()</an:docProponent>
						con il 
						<an:docProponent refersTo="#" as="#d85e77a1327">Ministro dell'economia e delle finanze ()</an:docProponent>
						con il
                                        
						<an:docProponent refersTo="#" as="#d85e79a1327">Ministro per gli affari regionali e le autonomie ()</an:docProponent>
						con il 
						<an:docProponent refersTo="#" as="#d85e82a1327">Ministro della salute ()</an:docProponent>
						e
                                    con il 
						<an:docProponent refersTo="#" as="#d85e84a1327">Ministro per la pubblica amministrazione e la semplificazione ()</an:docProponent>
					</an:p>
					
                                
					<an:p>
						<an:i>(</an:i>
