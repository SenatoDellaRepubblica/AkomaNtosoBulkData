				, 
				<an:docProponent refersTo="#d1972e28" as="#d1972e15a1327">MARTON</an:docProponent>
				, 
				<an:docProponent refersTo="#d1972e30" as="#d1972e15a1327">MONTEVECCHI</an:docProponent>
				 e 
				<an:docProponent refersTo="#d1972e32" as="#d1972e15a1327">SANTANGELO</an:docProponent>
