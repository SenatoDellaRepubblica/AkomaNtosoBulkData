				d'iniziativa dei senatori 
				<an:docProponent refersTo="#d1972e15" as="#d1972e15a1327">COTTI</an:docProponent>
				, 
				<an:docProponent refersTo="#d1972e17" as="#d1972e15a1327">BERTOROTTA</an:docProponent>
				, 
				<an:docProponent refersTo="#d1972e19" as="#d1972e15a1327">BUCCARELLA</an:docProponent>
				, 
				<an:docProponent refersTo="#d1972e21" as="#d1972e15a1327">CAPPELLETTI</an:docProponent>
				, 
				<an:docProponent refersTo="#d1972e23" as="#d1972e15a1327">FUCKSIA</an:docProponent>
