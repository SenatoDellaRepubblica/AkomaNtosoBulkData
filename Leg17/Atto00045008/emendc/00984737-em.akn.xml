				<an:docNumber>Emendamento n. 3.0.9</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#IDS550RL2ZCSW3CHQJUNUXY0LAXOTQA2VRXDXVJNHKFFLIQULWNR5H" as="#tipoSenatore">RUTA</an:docProponent>
