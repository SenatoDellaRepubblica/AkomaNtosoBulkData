			<an:p>
				<an:docProponent refersTo="#IDPVLSHFIJR5LNJPZ00NY5YW0G3OEBJ1HCGZL1ZZCRD1HOCEV5VSQK" as="#tipoSenatore">Sangalli, Parente, Lai</an:docProponent>
