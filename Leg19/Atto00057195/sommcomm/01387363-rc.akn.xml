		<an:debateBody title="Resoconto n.105 della legislatura 19">
			
         
			<!--Incontrata InizioSeduta-->
			<an:debateSection id="d1e42" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
