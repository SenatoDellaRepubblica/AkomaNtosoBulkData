		<an:debateBody title="Resoconto n.234 della legislatura 19">
			
         
			<an:debateSection id="d1e18" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="11:30:00"/>
