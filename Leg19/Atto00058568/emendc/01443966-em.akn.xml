			<an:amendmentContent>
				<an:p>
					<an:i>All'emendamento 6.0.100, comma 1, sopprimere le lettere</an:i>
					 b)
					<an:i>,</an:i>
					 e) 
					<an:i>e</an:i>
					 i)
					<an:i>.</an:i>
				</an:p>
				

				<an:p>
					    
					<an:i>Conseguentemente, al comma 4</an:i>
					:
				</an:p>
				

				<an:p>
					          1) 
					<an:i>sopprimere le seguenti parole:</an:i>
					 «
					<an:i>b)</an:i>
					» 
					<an:i>ed</an:i>
					 «
					<an:i>e)</an:i>
					»;
				</an:p>
				

				<an:p>
					          2) 
					<an:i>sostituire le parole:</an:i>
					 «
					<an:i>h)</an:i>
					 e 
					<an:i>i)</an:i>
					»
					<an:i>, con le seguenti:</an:i>
					 «e 
					<an:i>h)</an:i>
					».
				</an:p>
