			<an:p>
				<an:docProponent refersTo="#ID0E6" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">Minasi</an:span>
				</an:docProponent>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0EJB" as="#tipoSenatore">
