			<an:amendmentContent>
				<an:p style="margin-left:0cm; margin-right:0cm">
					<an:i>Alla tabella A</an:i>
					<an:i>, voce Ministero dell'Economia e delle Finanze</an:i>
					 
					<an:i>apportare le seguenti variazioni in diminuzione:</an:i>
				</an:p>
				

				<an:table border="0" cellpadding="0" cellspacing="0" style="border-collapse:collapse" id="ID0E3B">
					


					<an:tr>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:8.15pt; text-align:right">2026:</an:p>
							

						</an:td>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:351.5pt; text-align:right">- 1.000.000;</an:p>
							

						</an:td>
						

					</an:tr>
					

					<an:tr>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:8.15pt; text-align:right">2027:</an:p>
							

						</an:td>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:351.5pt; text-align:right">- 1.000.000;</an:p>
							

						</an:td>
						

					</an:tr>
					

					<an:tr>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:8.15pt; text-align:right">2028:</an:p>
							

						</an:td>
						

						<an:td>
							

							<an:p style="margin-left:0cm; margin-right:351.5pt; text-align:right">- 2.000.000.</an:p>
							

						</an:td>
						

					</an:tr>
					


				</an:table>
				

				<an:p style="margin-left:0cm; margin-right:0cm; text-align:justify">
					     
					<an:i>Conseguentemente, allo stato di previsione del Ministero delle Imprese e del Made in Italy</an:i>
					<an:i>, Missione 2 «</an:i>
					<an:i>Regolazione dei mercati»</an:i>
					<an:i>, programma 2.1 - «</an:i>
					<an:i>Vigilanza sui mercati e sui prodotti, promozione della concorrenza e tutela dei consumatori</an:i>
					<an:i>»,</an:i>
					 
					<an:i>apportare le seguenti variazioni:</an:i>
				</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">          2026:</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">
					          CP: + 1.000.000
					<an:eol/>
					
CS: + 1.000.000
				</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">          2027:</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">
					          CP: + 1.000.000
					<an:eol/>
					
CS: + 1.000.000
				</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">          2028:</an:p>
				

				<an:p style="margin-left:0cm; margin-right:0cm">
					          CP: + 2.000.000
					<an:eol/>
					
CS: + 2.000.000
				</an:p>
