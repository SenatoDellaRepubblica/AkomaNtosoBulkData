			<an:debateSection id="d1e18" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="21:30:00"/>
					La seduta inizia alle ore 21,30
				</an:narrative>
				
         
			</an:debateSection>
			
         
			<an:debateSection id="sede-d1e20" name="IN SEDE REFERENTE">
				
            
				<an:heading id="h-d1e20">IN SEDE REFERENTE</an:heading>
				
            
				<an:debateSection id="d-d1e23" name="1689 - Bilancio di previsione dello Stato per l'anno finanziario 2026 e bilancio pluriennale per il triennio 2026-2028">
					
               
					<an:heading id="h-d1e23"/>
					
               
					<an:block name="Atto">
						
                  
						<an:docType>DDL</an:docType>
						
                  
						<an:docNumber>1689</an:docNumber>
						
                  
						<an:docTitle>Bilancio di previsione dello Stato per l'anno finanziario 2026 e bilancio pluriennale per il triennio 2026-2028</an:docTitle>
						
                  
						<an:docProponent/>
						
                  
						<an:docStatus>
							
                     
							<an:eol/>
							Seguito e sospensione dell'esame
						</an:docStatus>
						
               
					</an:block>
					
               
					<an:narrative title="Fisionomia">
						
                  
						<an:b>(Tab.1)</an:b>
						 - Stato di previsione dell'entrata per l'anno finanziario 2026 e per il triennio 2026-2028 
						<an:i>(limitatamente alle parti di competenza)</an:i>
						
               
					</an:narrative>
					
               
					<an:narrative title="Fisionomia">
						
                  
						<an:b>(Tab.2) -</an:b>
						 Stato di previsione del Ministero dell'economia e delle finanze per l'anno finanziario 2026 e per il triennio 2026-2028 
						<an:i>(limitatamente alle parti di competenza)</an:i>
						
               
					</an:narrative>
					
               
					<an:narrative title="Fisionomia">            Prosegue l'esame sospeso nella seduta pomeridiana di oggi.</an:narrative>
					
               
					<an:speech by="#p34485" as="#d1e49a1081">
						
                  
						<an:from refersTo="#p34485"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e49" href="#p34485">PRESIDENTE</an:ref>
							 comunica che sono state presentate le riformulazioni 4.1000/26 (testo 2) e 4.1000/27 (testo 2), pubblicate in allegato.
						</an:p>
						
               
					</an:speech>
					
               
					<an:narrative title="Fisionomia">Comunica poi che è stato ritirato l'emendamento 46.0.6, e trasformato nell'ordine del giorno G/1689/64/5, pubblicato in allegato.</an:narrative>
					
               
					<an:narrative title="Fisionomia">Propone di convocare un Ufficio di Presidenza integrato dai rappresentanti dei Gruppi.</an:narrative>
					
               
					<an:narrative title="Fisionomia">La Commissione conviene.</an:narrative>
					
               
					<an:narrative title="Fisionomia">Il seguito dell'esame è quindi sospeso.</an:narrative>
					
            
				</an:debateSection>
				
            
				<an:p xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03" xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html"/>
				
         
			</an:debateSection>
			
         
			<an:debateSection id="sede-d1e56" name="CONVOCAZIONE DELL'UFFICIO DI PRESIDENZA INTEGRATO DAI RAPPRESENTANTI DEI GRUPPI">
				
            
				<an:heading id="h-d1e56">CONVOCAZIONE DELL'UFFICIO DI PRESIDENZA INTEGRATO DAI RAPPRESENTANTI DEI GRUPPI</an:heading>
				
            
				<an:debateSection id="d-d1e59" name="">
					
               
					<an:heading id="h-d1e59"/>
					
               
					<an:speech by="#p34485" as="#d1e49a1081">
						
                  
						<an:from refersTo="#p34485"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e65" href="#p34485">PRESIDENTE</an:ref>
							 comunica che è immediatamente convocato l'Ufficio di Presidenza integrato dai rappresentanti dei Gruppi.
						</an:p>
						
               
					</an:speech>
					
               
					<an:narrative title="Fisionomia">La Commissione prende atto.</an:narrative>
					
               
					<an:narrative title="Fisionomia">
						
                  
						<an:i>La seduta, sospesa alle ore 21,35, riprende alle ore 00,25.</an:i>
						
               
					</an:narrative>
					
            
				</an:debateSection>
				
            
				<an:p xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03" xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html"/>
				
         
			</an:debateSection>
			
         
			<an:debateSection id="sede-d1e71" name="IN SEDE REFERENTE">
				
            
				<an:heading id="h-d1e71">IN SEDE REFERENTE</an:heading>
				
            
				<an:debateSection id="d-d1e74" name="1689 - Bilancio di previsione dello Stato per l'anno finanziario 2026 e bilancio pluriennale per il triennio 2026-2028">
					
               
					<an:heading id="h-d1e74"/>
					
               
					<an:block name="Atto">
						
                  
						<an:docType>DDL</an:docType>
						
                  
						<an:docNumber>1689</an:docNumber>
						
                  
						<an:docTitle>Bilancio di previsione dello Stato per l'anno finanziario 2026 e bilancio pluriennale per il triennio 2026-2028</an:docTitle>
						
                  
						<an:docProponent/>
						
                  
						<an:docStatus>
							
                     
							<an:eol/>
							Ripresa dell'esame e rinvio
						</an:docStatus>
						
               
					</an:block>
					
               
					<an:narrative title="Fisionomia">Prosegue l'esame precedentemente sospeso.</an:narrative>
					
               
					<an:speech by="#p34485" as="#d1e49a1081">
						
                  
						<an:from refersTo="#p34485"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e92" href="#p34485">PRESIDENTE</an:ref>
							 dà la parola al sottosegretario FRENI.
						</an:p>
						
               
					</an:speech>
					
               
					<an:narrative title="Fisionomia">
						Il sottosegretario FRENI dichiara che, per venire incontro a tutte le sensibilità politiche e alle esigenze di carattere tecnico, il Governo ritira l'emendamento 4.1000 e preannuncia la presentazione - entro qualche ora - di una versione ridotta, contenente le parti relative al PNRR e all'
						<an:i>iper</an:i>
						-ammortamento, con riserva di valutare la parte relativa alla ZES unica per le imprese del settore agricolo.
					</an:narrative>
					
               
					<an:narrative title="Fisionomia">Osserva inoltre che, in tal modo, potranno eventualmente farsi salvi i subemendamenti già presentati, riferiti alle parti dell'emendamento 4.1000 riprodotte nella nuova proposta del Governo.</an:narrative>
					
               
					<an:narrative title="Fisionomia">La Commissione prende atto.</an:narrative>
					
               
					<an:speech by="#p32681" as="#senatore">
						
                  
						<an:from refersTo="#p32681"/>
						
                  
						<an:p>
							Il senatore 
							<an:ref id="d1e104" href="#p32681">PATUANELLI</an:ref>
							 (
							<an:i>M5S</an:i>
							) dichiara, a nome del proprio Gruppo, che in ogni caso non risulta necessario stabilire un termine per la presentazione di subemendamenti.
						</an:p>
						
                  
						<an:p>Il seguito dell'esame è quindi rinviato.</an:p>
						
               
					</an:speech>
					
            
				</an:debateSection>
				
            
				<an:p xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03" xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html"/>
				
         
			</an:debateSection>
			
         
			<an:debateSection id="sede-d1e110" name="ANTICIPAZIONE DELLA SEDUTA ANTIMERIDIANA DI DOMANI">
				
            
				<an:heading id="h-d1e110">ANTICIPAZIONE DELLA SEDUTA ANTIMERIDIANA DI DOMANI</an:heading>
				
            
				<an:debateSection id="d-d1e113" name="">
					
               
					<an:heading id="h-d1e113"/>
					
               
					<an:speech by="#p34485" as="#d1e49a1081">
						
                  
						<an:from refersTo="#p34485"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e119" href="#p34485">PRESIDENTE</an:ref>
							 comunica che la seduta plenaria, già convocata alle ore 9 di domani, venerdì 19 dicembre 2025, è anticipata alle ore 8.
						</an:p>
						
               
					</an:speech>
					
               
					<an:narrative title="Fisionomia">La Commissione prende atto.</an:narrative>
					
               
					<an:narrative title="Fisionomia">
						
                  
						<an:i>La seduta termina alle ore 00,35.</an:i>
						
               
					</an:narrative>
					
            
				</an:debateSection>
				
            
				<an:p xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03" xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html"/>
				
         
			</an:debateSection>
			
         
			<an:debateSection id="d1e125" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="00:35:00"/>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
		</an:debateBody>
		
   
