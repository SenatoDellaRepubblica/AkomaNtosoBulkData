			<an:container id="w17aab1b1b5" name="Sezione">
				<an:p>
					PER IL
					<an:eol/>
					<an:b>DISEGNO DI LEGGE</an:b>
				</an:p>
				<an:container id="w17aab1b1b5b1" name="Atto">
					<an:p>
						<an:docNumber/>
					</an:p>
					<an:p>
						<an:docTitle>Ratifica ed esecuzione dell'Accordo in materia di coproduzione cinematografica tra il Governo della Repubblica italiana e il Governo del Giappone, con Allegato, fatto a Tokyo il 28 giugno 2023</an:docTitle>
					</an:p>
					<an:p>
						presentato dal 
						<an:docProponent refersTo="#" as="#w17aab1b1b5b1b1aaab1Aa">Ministro degli affari esteri e della cooperazione internazionale ()</an:docProponent>
						di concerto con il 
						<an:docProponent refersTo="#" as="#w17aab1b1b5b1b1aaab3Aa">Ministro dell'economia e delle finanze ()</an:docProponent>
						e con il 
						<an:docProponent refersTo="#" as="#w17aab1b1b5b1b1aaab5Aa">Ministro della cultura ()</an:docProponent>
					</an:p>
					<an:p>COMUNICATO ALLA PRESIDENZA IL 15 DICEMBRE 2023</an:p>
				</an:container>
			</an:container>
			<an:p>
				<an:docTitle/>
			</an:p>
