				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="11:10:00"/>
					
               
					<an:i> La seduta termina alle ore 11,10.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
