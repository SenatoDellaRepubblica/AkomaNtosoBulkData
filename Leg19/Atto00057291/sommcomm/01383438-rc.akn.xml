				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="14:30:00"/>
					
               
					<an:i> La seduta termina alle ore 14,30.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
