			<an:p>
				<an:docNumber>Emendamento n. 10.0.603/4 (testo 2)</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E6" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">De</an:span>
					 
					<an:span style="font-variant:small-caps;">Priamo</an:span>
				</an:docProponent>
			</an:p>
