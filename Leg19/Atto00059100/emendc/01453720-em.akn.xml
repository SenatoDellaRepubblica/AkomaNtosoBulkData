		<an:amendmentBody>
			<an:amendmentContent>
				<an:p style="margin-bottom:13.3pt;text-align:justify;line-height:150%;">
					<an:i>All'articolo apportare le seguenti modificazioni:</an:i>
				</an:p>
				

				<an:p style="margin-bottom:13.3pt;text-align:justify;line-height:150%;">
					          
					<an:i>a)</an:i>
					 
					<an:i>al comma 1, sopprimere il secondo periodo;</an:i>
				</an:p>
				

				<an:p style="margin-bottom:13.3pt;text-align:justify;line-height:150%;">
					          
					<an:i>b)</an:i>
					 
					<an:i>al comma 2:</an:i>
				</an:p>
				

				<an:p style="margin-top:0cm;margin-right:0cm;margin-bottom:13.3pt;margin-left:54.0pt;text-align:justify;line-height:150%;">
					          
					<an:i>1)</an:i>
					 
					<an:i>dopo le parole:</an:i>
					 &quot;provvedimenti di riorganizzazione,&quot; 
					<an:i>inserire le seguenti:</an:i>
					 &quot;ai sensi dell'articolo 17 della legge 23 agosto 1988, n. 400,&quot;;
				</an:p>
				

				<an:p style="margin-top:0cm;margin-right:0cm;margin-bottom:13.3pt;margin-left:54.0pt;text-align:justify;line-height:150%;">
					          
					<an:i>2)</an:i>
					 
					<an:i>sopprimere le parole:</an:i>
					 &quot;non generali&quot;;
				</an:p>
				

				<an:p style="margin-bottom:13.3pt;text-align:justify;line-height:150%;">
					          
					<an:i>c)</an:i>
					 
					<an:i>sopprimere il comma 3.</an:i>
				</an:p>
			</an:amendmentContent>
		</an:amendmentBody>
