				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="09:30:00"/>
					
               
					<an:i>La seduta termina alle ore 9,30.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
