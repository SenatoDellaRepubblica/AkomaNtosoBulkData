				<an:affectedDocument href="#ar1">Ddl 1433</an:affectedDocument>
			</an:p>
			<an:p>
				<an:docNumber>Emendamento n. Coord.2</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E5">
					<an:span style="font-variant:small-caps;">Le</an:span>
					 
					<an:span style="font-variant:small-caps;">Relatrici</an:span>
				</an:docProponent>
			</an:p>
