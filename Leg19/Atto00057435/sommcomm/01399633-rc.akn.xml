				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="19:40:00"/>
					
               
					<an:i>La seduta termina alle ore 19,40.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
