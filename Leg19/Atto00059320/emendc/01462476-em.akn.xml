		<an:amendmentBody>
			<an:amendmentContent>
				<an:p style="text-align:justify">
					<an:i>All'articolo 1:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 1:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>                   al primo periodo, dopo le parole:</an:i>
					 «ILVA S.p.A. in amministrazione straordinaria» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>al secondo periodo, sostituire le parole:</an:i>
					 «ILVA S.p.A. in amministrazione straordinaria, può» 
					<an:i>con le seguenti:</an:i>
					 «La società ILVA S.p.A. in amministrazione straordinaria può» 
					<an:i>e le parole:</an:i>
					 «a Acciaierie d'Italia» 
					<an:i>con le seguenti:</an:i>
					 «alla società Acciaierie d'Italia».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 5:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 2, dopo le parole:</an:i>
					 «contratto di cui al comma 1» 
					<an:i>sopprimere il segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                   
					<an:i>al comma 4, dopo le parole:</an:i>
					 «l'offerta» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 6:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 1, dopo le parole:</an:i>
					 «per l'anno 2025» 
					<an:i>sopprimere il segno di interpunzione:</an:i>
					 «,»
					<an:i>, dopo le parole:</an:i>
					 «di cui al comma 1» 
					<an:i>sopprimere il segno di interpunzione:</an:i>
					 «,» 
					<an:i>e sostituire le parole:</an:i>
					 «di cui al decreto legislativo» 
					<an:i>con le seguenti:</an:i>
					 «del medesimo decreto legislativo».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 7:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 1, sostituire le parole:</an:i>
					 «con un numero di lavoratori dipendenti complessivamente non inferiore a mille unità impiegati sul territorio italiano» 
					<an:i>con le seguenti:</an:i>
					 «con un numero di lavoratori dipendenti impiegati sul territorio italiano complessivamente non inferiore a mille unità»;
				</an:p>
				

				<an:p style="text-align:justify">
					                   
					<an:i>al comma 2:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>                   all'alinea, sostituire le parole:</an:i>
					 «2025, 31,3 milioni» 
					<an:i>con le seguenti:</an:i>
					 «2025, a 31,3 milioni» 
					<an:i>e le parole:</an:i>
					 «2026, 0,9 milioni» 
					<an:i>con le seguenti:</an:i>
					 «2026, in 0,9 milioni»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>alla lettera</an:i>
					 a)
					<an:i>, sostituire le parole:</an:i>
					 «2025, 31,3 milioni» 
					<an:i>con le seguenti:</an:i>
					 «2025, a 31,3 milioni» 
					<an:i>e dopo le parole:</an:i>
					 «per l'anno 2027» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>alla lettera</an:i>
					 b)
					<an:i>, dopo le parole:</an:i>
					 «per l'anno 2027» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>alla lettera</an:i>
					 c)
					<an:i>, dopo le parole:</an:i>
					 «per l'anno 2028» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 8:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 1:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>                   al capoverso 1-</an:i>
					ter
					<an:i>, primo periodo, dopo le parole:</an:i>
					 «cessazione di attività» 
					<an:i>sopprimere il segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>al capoverso 1-</an:i>
					quinquies
					<an:i>, terzo periodo, sostituire le parole:</an:i>
					 «entro 60 giorni dall'entrata» 
					<an:i>con le seguenti:</an:i>
					 «entro sessanta giorni dalla data di entrata».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 9:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         alla rubrica, sostituire la parola:</an:i>
					 «Modifiche» 
					<an:i>con la seguente:</an:i>
					 «Modifica».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 10:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 1:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>                   all'alinea, dopo le parole:</an:i>
					 «All'articolo 2» 
					<an:i>sopprimere il segno di interpunzione:</an:i>
					 «,»;
				</an:p>
				

				<an:p style="text-align:justify">
					                             
					<an:i>alla lettera</an:i>
					 b)
					<an:i>, numero 2), dopo le parole:</an:i>
					 «Il datore di lavoro» 
					<an:i>inserire il seguente segno di interpunzione:</an:i>
					 «,».
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>All'articolo 11:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>         al comma 2, lettera</an:i>
					 a)
					<an:i>, sostituire le parole:</an:i>
					 «comma 3 e 7, comma 2» 
					<an:i>con le seguenti:</an:i>
					 «comma 3, e 7, comma 2,».
				</an:p>
