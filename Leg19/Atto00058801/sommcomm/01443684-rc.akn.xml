		<an:debateBody title="Resoconto n.205 della legislatura 19">
			
         
			<an:debateSection id="d1e18" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="09:40:00"/>
