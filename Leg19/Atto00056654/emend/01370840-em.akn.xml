			<an:p>
				<an:docProponent refersTo="#IDFQ3FD1SXVT0UM41GJZVO1T3EHNSCJR3UGXD4H4K3FQYOI0XZ1ROC" as="#tipoSenatore">LOMBARDO</an:docProponent>
