			<an:p>
				<an:docProponent refersTo="#IDODLZCRECS2CGKQL53MECXEUN5DTHUPGQ4TJOAAOEEUILK4P3TQGM" as="#tipoSenatore">DI GIROLAMO</an:docProponent>
