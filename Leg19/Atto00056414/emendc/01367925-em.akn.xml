			<an:p>
				<an:docNumber>Emendamento n. 1.0.7</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E5" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">Di</an:span>
					 
					<an:span style="font-variant:small-caps;">Girolamo</an:span>
