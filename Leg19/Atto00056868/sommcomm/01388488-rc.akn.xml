				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="09:35:00"/>
					
               
					<an:i>La seduta termina alle ore 9,35.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
