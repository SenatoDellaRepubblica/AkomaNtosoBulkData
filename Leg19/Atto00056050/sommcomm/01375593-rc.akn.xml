				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="14:05:00"/>
					
               
					<an:i>La seduta termina alle ore 14,05.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
