		<an:amendmentBody>
			<an:amendmentContent>
				<an:p>
					<an:i>Apportare le seguenti modificazioni:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					         
					<an:i> a) al comma 3, lettera a), sopprimere le parole:</an:i>
					 «nell'ambito degli strumenti regionali di pianificazione del territorio»;
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>b) al comma 5, lettera a), sostituire la parola:</an:i>
					 «g)» 
					<an:i>con la seguente:</an:i>
					 «c)»;
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>c) al comma 5, lettera d):</an:i>
				</an:p>
				

				<an:p style="margin-left:72pt; margin-right:0cm; text-align:justify">
					          
					<an:i>1) sostituire le parole:</an:i>
					 «novanta giorni»  
					<an:i>con le seguenti:</an:i>
					 «centottanta giorni»;
				</an:p>
				

				<an:p style="margin-left:72pt; margin-right:0cm; text-align:justify">
					          
					<an:i>2) sostituire le parole:</an:i>
					 «la riduzione» 
					<an:i>con le seguenti:</an:i>
					 «l'eventuale riduzione».
				</an:p>
			</an:amendmentContent>
		</an:amendmentBody>
