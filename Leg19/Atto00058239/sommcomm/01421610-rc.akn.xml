				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="15:45:00"/>
					
               
					<an:i>La seduta termina alle ore 15,45.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
		</an:debateBody>
		
      
