			<an:p>
				<an:docNumber>Emendamento n. Coord.1 (testo 2)</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E6">
					<an:span style="font-variant:small-caps;">Il</an:span>
					 
					<an:span style="font-variant:small-caps;">Relatore</an:span>
				</an:docProponent>
			</an:p>
