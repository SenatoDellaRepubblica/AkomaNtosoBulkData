		<an:amendmentBody>
			<an:amendmentContent>
				<an:p style="text-align:justify">
					<an:i>Al comma 1, lettera</an:i>
					 b)
					<an:i>, numero 2-</an:i>
					bis)
					<an:i>, capoverso</an:i>
					 «
					<an:i>4.</an:i>
					»,
					<an:i> apportare le seguenti modificazioni:</an:i>
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>a)</an:i>
					 
					<an:i>al primo periodo, sopprimere le parole:</an:i>
					 &quot;gravi e&quot;;
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>b)</an:i>
					 
					<an:i>al quarto periodo, sostituire le parole:</an:i>
					 &quot;tre giorni&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;sette giorni&quot;;
				</an:p>
				

				<an:p style="text-align:justify">
					          
					<an:i>c)</an:i>
					 
					<an:i>all'ultimo periodo, sostituire le parole:</an:i>
					 &quot;Nei casi previsti dalle lettere 
					<an:i>b)</an:i>
					, 
					<an:i>c)</an:i>
					 e 
					<an:i>d)</an:i>
					 del comma 3&quot; 
					<an:i>con le seguenti:</an:i>
					 &quot;Nei casi previsti dal comma 3&quot;.
				</an:p>
			</an:amendmentContent>
		</an:amendmentBody>
