				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="14:50:00"/>
					
               
					<an:i>La seduta termina alle ore 14,50.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
