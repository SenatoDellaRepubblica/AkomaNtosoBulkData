					<an:references source="#redattore">
						<an:original id="ro2" href="http://dati.senato.it/osr/Ddl/2022-11-16/328/ita@/main/relazione" showAs="Relazione"/>
						<an:attachmentOf id="mainAnx2" href="http://dati.senato.it/osr/Ddl/2022-11-16/328/ita@/main" showAs="Main"/>
