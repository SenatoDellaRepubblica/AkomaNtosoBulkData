				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="08:55:00"/>
					
               
					<an:i> La seduta termina alle ore 8,55.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
