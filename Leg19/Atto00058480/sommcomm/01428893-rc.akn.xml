		<an:debateBody title="Resoconto n.12 della legislatura 19">
			
         
			<an:debateSection id="d1e38" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="12:05:00"/>
					
               
					<an:i> La seduta inizia alle ore 12,05.</an:i>
					
            
