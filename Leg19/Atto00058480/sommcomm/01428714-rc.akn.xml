				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="18:40:00"/>
					
               
					<an:i> La seduta termina alle ore 18,40.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
