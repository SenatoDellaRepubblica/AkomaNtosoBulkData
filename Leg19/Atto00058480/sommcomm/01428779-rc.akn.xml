			</an:debateSection>
			
         
			<an:debateSection id="d1e590" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="16:20:00"/>
					
               
					<an:i>La seduta termina alle ore 16,20.</an:i>
					
            
				</an:narrative>
				
         
