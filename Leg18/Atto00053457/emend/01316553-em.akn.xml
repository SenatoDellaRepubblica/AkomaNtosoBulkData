				<an:docNumber>Proposta di non passaggio agli articoli n. NP1</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#IDYAATKCNGG5XTDMESFKON30YKABUJGQKO0OPOH4BQTH4CERXXZAZH" as="#tipoSenatore">CALDEROLI</an:docProponent>
