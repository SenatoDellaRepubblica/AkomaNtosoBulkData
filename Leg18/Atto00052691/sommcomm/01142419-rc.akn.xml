			<an:debateSection id="sede-d1e43" name="INTEGRAZIONE DELL'UFFICIO DI PRESIDENZA">
				
            
				<an:heading id="h-d1e43">INTEGRAZIONE DELL'UFFICIO DI PRESIDENZA</an:heading>
				
            
            
				<an:debateSection id="d-d1e51" name="">
					
               
					<an:heading id="h-d1e51"/>
					
                
               
					<an:narrative title="Fisionomia">
						
                  
						<an:b>Elezione di un Vice Presidente</an:b>
						
               
					</an:narrative>
					
               
					<an:narrative title="Fisionomia"> La Commissione procede all'elezione di un Vice Presidente.</an:narrative>
					
               
					<an:speech by="#p32582" as="#senatore">
						
                  
						<an:from refersTo="#p32582"/>
						
                  
						<an:p>
							Prendono parte alla votazione il presidente 
							<an:ref id="d1e77" href="#p32582">
                        BAGNAI
                     </an:ref>
							 (
							<an:i>L-SP-PSd'Az</an:i>
							) e i senatori Laura 
							<an:ref id="d1e86" href="#p29053">
                        BOTTICI
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), 
							<an:ref id="d1e95" href="#p29055">
                        BUCCARELLA
                     </an:ref>
							 (
							<an:i>Misto</an:i>
							), 
							<an:ref id="d1e105" href="#p32610">
                        D'ALFONSO
                     </an:ref>
							 (
							<an:i>PD</an:i>
							), 
							<an:ref id="d1e114" href="#p32621">
                        DI NICOLA
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), Tiziana Carmela Rosaria 
							<an:ref id="d1e123" href="#p32623">
                        DRAGO
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), 
							<an:ref id="d1e133" href="#p32630">
                        FENU
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), 
							<an:ref id="d1e142" href="#p17211">
                        LANNUTTI
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), Cinzia 
							<an:ref id="d1e151" href="#p32652">
                        LEONE
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							), 
							<an:ref id="d1e161" href="#p21507">
                        MARINO
                     </an:ref>
							 (
							<an:i>IV-PSI</an:i>
							), 
							<an:ref id="d1e170" href="#p29138">
                        MIRABELLI
                     </an:ref>
							 (
							<an:i>PD</an:i>
							) in sostituzione del senatore Cerno, 
							<an:ref id="d1e179" href="#p23015">
                        MONTANI
                     </an:ref>
							 (
							<an:i>L-SP-PSd'Az</an:i>
							), 
							<an:ref id="d1e189" href="#p32675">
                        ORTIS
                     </an:ref>
