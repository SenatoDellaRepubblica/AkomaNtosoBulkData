				<an:docProponent refersTo="#w1aaab1b2aab1ad259" as="#w1aaab1b2aab1ab1a">Marco DI MAIO</an:docProponent>
				, 
				<an:docProponent refersTo="#w1aaab1b2aab1ad261" as="#w1aaab1b2aab1ab1a">Gavino MANCA</an:docProponent>
				, 
				<an:docProponent refersTo="#w1aaab1b2aab1ad263" as="#w1aaab1b2aab1ab1a">INCERTI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1aaab1b2aab1ad265" as="#w1aaab1b2aab1ab1a">CANTINI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1aaab1b2aab1ad267" as="#w1aaab1b2aab1ab1a">FASSINO</an:docProponent>
				 e 
				<an:docProponent refersTo="#w1aaab1b2aab1ad269" as="#w1aaab1b2aab1ab1a">CARNEVALI</an:docProponent>
				 (1221); 
				<an:docProponent refersTo="#w1aaab1b2aab1ad271" as="#w1aaab1b2aab1ab1a">RAMPELLI</an:docProponent>
				, 
				<an:docProponent refersTo="#w1aaab1b2aab1ad273" as="#w1aaab1b2aab1ab1a">MOLLICONE</an:docProponent>
				 e 
				<an:docProponent refersTo="#w1aaab1b2aab1ad275" as="#w1aaab1b2aab1ab1a">FRASSINETTI</an:docProponent>
				 (1222)
			</an:p>
			<an:p>
				(V. 
				<an:i>Stampati Camera nn. </an:i>
				<an:b>523, 784, 914, 1221</an:b>
				<an:i> e </an:i>
				<an:b>1222</an:b>
				)
			</an:p>
			<an:p>
				<an:i>Trasmesso dal Presidente della Camera dei deputati alla Presidenza</an:i>
