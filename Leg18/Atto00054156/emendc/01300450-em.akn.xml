							<an:p style="text-align:center">
								<an:strong>LIVELLI DI ISEE</an:strong>
							</an:p>
							

						</an:td>
						

						<an:td colspan="2">
							

							<an:p style="text-align:center">
								<an:strong>IMPORTI MENSILI PER CIASCUN FIGLIO MINORE</an:strong>
							</an:p>
							

						</an:td>
						

					</an:tr>
					

					<an:tr>
						

						<an:td>
							

							<an:p style="text-align:center">
								<an:strong>
									<an:i>Nuclei fino a due figli minori</an:i>
								</an:strong>
							</an:p>
							

						</an:td>
						

