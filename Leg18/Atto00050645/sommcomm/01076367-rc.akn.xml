				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="14:40:00"/>
					
               
					<an:i>La seduta termina alle ore 14,40.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
