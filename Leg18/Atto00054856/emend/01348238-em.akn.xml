			<an:p>
				<an:docProponent refersTo="#IDJ4FJK3HT50DDIJA4R4AYZC2ZGMBZBJVUNKWZTNK0DZS0RMGSDWPB" as="#tipoSenatore">VITALI</an:docProponent>
