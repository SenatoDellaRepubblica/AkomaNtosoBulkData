			</an:debateSection>
			
         
			<an:debateSection id="sede-d1e239" name="POSTICIPAZIONE DELLA SEDUTA DI DOMANI">
				
            
				<an:heading id="h-d1e239">POSTICIPAZIONE DELLA SEDUTA DI DOMANI</an:heading>
				
            
            
				<an:debateSection id="d-d1e247" name="">
					
               
					<an:heading id="h-d1e247"/>
					
                
               
					<an:speech by="#p32610" as="#d1e95a1327">
						
                  
						<an:from refersTo="#p32610"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e263" href="#p32610">
                        PRESIDENTE
                     </an:ref>
							 comunica che la seduta di domani, mercoledì 4 maggio, già convocata alle ore 14, è posticipata alle ore 19.
						</an:p>
						
                  
						<an:p>Prendono atto le Commissioni riunite.</an:p>
						

               
					</an:speech>
					
            
				</an:debateSection>
				
            
