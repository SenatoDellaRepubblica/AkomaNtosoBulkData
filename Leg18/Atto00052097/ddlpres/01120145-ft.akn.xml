					<an:references source="#redattore">
						<an:original href="http://dati.senato.it/osr/Ddl/2019-07-23/1428/ita@/main/relazione" id="ro2" showAs="Relazione"/>
						<an:attachmentOf href="http://dati.senato.it/osr/Ddl/2019-07-23/1428/ita@/main" id="mainAnx2" showAs="Main"/>
					</an:references>
