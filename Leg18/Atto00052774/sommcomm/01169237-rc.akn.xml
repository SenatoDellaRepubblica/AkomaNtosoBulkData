			</an:debateSection>
			
         
			<an:debateSection id="d1e495" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="15:20:00"/>
					
               
					<an:i>La seduta termina alle ore 15,20.</an:i>
					
            
				</an:narrative>
				
         
