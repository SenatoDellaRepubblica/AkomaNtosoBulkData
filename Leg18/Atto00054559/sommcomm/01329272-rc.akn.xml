			</an:debateSection>
			
         
			<an:debateSection id="d1e174" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="10:20:00"/>
					
               
					<an:i>La seduta termina alle ore 10,20.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
