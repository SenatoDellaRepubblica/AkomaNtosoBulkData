				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="15:40:00"/>
					
               
					<an:i> La seduta termina alle ore 15,40.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
