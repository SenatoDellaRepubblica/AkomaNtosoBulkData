			<an:p>
				<an:docNumber>Emendamento n. 50.58 (testo 2)</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0E6">
					<an:span style="font-variant:small-caps;">Le</an:span>
					 
					<an:span style="font-variant:small-caps;">Commissioni</an:span>
					 
					<an:span style="font-variant:small-caps;">riunite</an:span>
				</an:docProponent>
			</an:p>
