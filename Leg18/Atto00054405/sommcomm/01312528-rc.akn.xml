		<an:debateBody title="Resoconto n.243 della legislatura 18">
			
         
			<an:debateSection id="d1e37" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="21:00:00"/>
					
               
