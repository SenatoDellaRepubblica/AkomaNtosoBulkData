					<an:p style="text-align:right;">
						<an:i>(articolo 1)</an:i>
					</an:p>
					<an:p style="text-align:center;">
						<an:b>ELENCO DELLE ISOLE MINORI</an:b>
					</an:p>
					<an:table border="1" frame="box" id="w1aaab4b3">
						<an:tr>
							<an:th colspan="1" rowspan="1">
								<an:p>
									<an:b>Prov.</an:b>
								</an:p>
							</an:th>
							<an:th colspan="1" rowspan="1">
								<an:p>
									<an:b>Isola</an:b>
								</an:p>
							</an:th>
							<an:th colspan="1" rowspan="1">
								<an:p>
									<an:b>Abit.</an:b>
								</an:p>
							</an:th>
