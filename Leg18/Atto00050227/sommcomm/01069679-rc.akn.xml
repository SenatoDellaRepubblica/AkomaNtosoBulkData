			<an:documentCollection name="Allegato">
				
            
				<an:meta>
					
               
					<an:identification xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html" xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03" source="#redattore">
						
                  
