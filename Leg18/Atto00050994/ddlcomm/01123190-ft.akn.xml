						, 
						<an:docProponent refersTo="#w1ab1ab1b5b1b1aaab7" as="#w1ab1ab1b5b1b1aaab1a">FERRARA</an:docProponent>
						, 
						<an:docProponent refersTo="#w1ab1ab1b5b1b1aaab9" as="#w1ab1ab1b5b1b1aaab5a">senatrice (PACIFICO)</an:docProponent>
						, 
						<an:docProponent refersTo="#w1ab1ab1b5b1b1aaac11" as="#w1ab1ab1b5b1b1aaab1a">PETROCELLI</an:docProponent>
						 e 
						<an:docProponent refersTo="#w1ab1ab1b5b1b1aaac13" as="#w1ab1ab1b5b1b1aaab5a">senatrice (VANIN)</an:docProponent>
					</an:p>
