					<an:heading id="h-d1e178"/>
					
                
               
					<an:speech by="#p29115" as="#d1e194a1327">
						
                  
						<an:from refersTo="#p29115"/>
						
                  
						<an:p>
							 Il 
							<an:ref id="d1e194" href="#p29115">
                        PRESIDENTE
                     </an:ref>
							 comunica che la seduta, già prevista alle ore 8,45 di domani, giovedì 23 giugno 2022, non avrà luogo.
						</an:p>
						

               
					</an:speech>
					
            
