			<an:p>
				<an:docProponent refersTo="#IDG1A5B2UGUKUDNNKL2FUQGYQKBBZKTW3BQKM03MPCYD4IMFPFXLAI" as="#tipoSenatore">DE BERTOLDI</an:docProponent>
