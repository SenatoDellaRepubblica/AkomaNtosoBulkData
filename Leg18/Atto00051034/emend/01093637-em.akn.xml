			<an:p>
				<an:docProponent as="#tipoSenatore" refersTo="#IDG1A5B2UGUKUDNNKL2FUQGYQKBBZKTW3BQKM03MPCYD4IMFPFXLAI">DE BERTOLDI</an:docProponent>
