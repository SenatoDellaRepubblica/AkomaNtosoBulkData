			<an:debateSection id="d1e38" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="13:45:00"/>
					
               
					<an:i> La seduta inizia alle ore 13,45.</an:i>
					
            
				</an:narrative>
				
         
