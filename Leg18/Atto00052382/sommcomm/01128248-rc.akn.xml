			</an:debateSection>
			
         
			<an:debateSection id="d1e520" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="16:35:00"/>
					
               
					<an:i>La seduta termina alle ore 16,35.</an:i>
					
            
				</an:narrative>
				
         
