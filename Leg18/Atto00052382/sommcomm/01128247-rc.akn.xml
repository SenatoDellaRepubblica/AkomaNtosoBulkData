			</an:debateSection>
			
         
			<an:debateSection id="d1e482" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="10:40:00"/>
					
               
					<an:i>La seduta termina alle ore 10,40.</an:i>
					
            
				</an:narrative>
				
         
