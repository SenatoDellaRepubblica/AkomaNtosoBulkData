					<an:narrative title="Fisionomia">
						
                  
						<an:i>Prosegue l'esame, sospeso nella prima seduta pomeridiana di oggi.</an:i>
						
               
					</an:narrative>
					
               
					<an:speech by="#p32604" as="#d1e96a1327">
						
                  
						<an:from refersTo="#p32604"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e96" href="#p32604">
                        PRESIDENTE
                     </an:ref>
							 comunica che sono state presentate le seguenti riformulazioni, pubblicate in allegato: 1.33 (testo 2), 1.39 (testo 2), 1.71 (testo 2), 1.87 (testo 2), 1.120 (testo 2), 1.121 (testo 2), 1.229 (testo 3), 1.141 (testo 2), 1.149 (testo 2), 1.241 (testo 2), 1.245 (testo 2), 1.249 (testo 2), 1.265 (testo 2), 1.275 (testo 2) e 1.307 (testo 2).
						</an:p>
						
               
					</an:speech>
					
               
