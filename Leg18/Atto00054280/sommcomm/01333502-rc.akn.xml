						<an:from refersTo="#p32619"/>
						
                  
						<an:p>
							La senatrice 
							<an:ref id="d1e108" href="#p32619">
                        DI GIROLAMO
                     </an:ref>
							 (
							<an:i>M5S</an:i>
							) ritira gli emendamenti 1.133, 1.238, 1.260, 1.302, 1.303 e 1.314.
						</an:p>
						
               
					</an:speech>
					
               
