					<an:references source="#redattore">
						<an:original href="http://dati.senato.it/osr/Ddl/2020-09-17/1937/ita@/main/relazione" id="ro2" showAs="Relazione"/>
						<an:attachmentOf href="http://dati.senato.it/osr/Ddl/2020-09-17/1937/ita@/main" id="mainAnx2" showAs="Main"/>
