				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="14:15:00"/>
					
               
					<an:i> La seduta termina alle ore 14,15.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
