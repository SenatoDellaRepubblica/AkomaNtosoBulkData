						<an:tr>
							
                  
							<an:td colspan="1" rowspan="1">
								
                     
                        
								<an:p>
									
                           
									<an:i>a)</an:i>
									 Trattato di estradizione tra la Repubblica italiana e la Repubblica dominicana, fatto a Roma il 13 febbraio 2019;
								</an:p>
								
                     
                  
