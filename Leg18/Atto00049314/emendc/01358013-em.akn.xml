				<an:docProponent refersTo="#ID0E5" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">Marino</an:span>
				</an:docProponent>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#ID0EIB" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">Cucca</an:span>
				</an:docProponent>
			</an:p>
