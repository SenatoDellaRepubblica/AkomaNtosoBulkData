				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="12:10:00"/>
					
               
					<an:i> La seduta termina alle ore 12,10.</an:i>
					
            
				</an:narrative>
				
         
			</an:debateSection>
			
      
