					<an:block name="Atto">
						
                  
						<an:docType>DDL</an:docType>
						
                  
						<an:docNumber>1634</an:docNumber>
						
                  
						<an:docTitle>
                           
                              Disposizioni in materia di revisione del modello organizzativo del Sistema di emergenza sanitaria territoriale «118»
                           
                        </an:docTitle>
						
                  
						<an:docProponent/>
						
                  
						<an:docStatus/>
						
               
