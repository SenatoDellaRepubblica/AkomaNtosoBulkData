					<an:speech by="#p29521" as="#d1e207a1327">
						
                  
						<an:from refersTo="#p29521"/>
						
                  
						<an:p>
							Il 
							<an:ref id="d1e277" href="#p29521">
                        PRESIDENTE
                     </an:ref>
							 avverte che la seduta già convocata pe le ore 8,45 di domani, 5 novembre, non avrà luogo.
						</an:p>
						
                  
						<an:p>La Commissione prende atto.</an:p>
						

                  
						<an:p>
							
                     
							<an:i>
								
                        
								<an:eol/>
								 
                     
							</an:i>
							
                  
						</an:p>
						
               
					</an:speech>
					
            
