			<an:p>
				<an:docProponent refersTo="#ID0E5" as="#tipoSenatore">
					<an:span style="font-variant:small-caps;">Garnero</an:span>
					 
					<an:span style="font-variant:small-caps;">Santanchè</an:span>
				</an:docProponent>
