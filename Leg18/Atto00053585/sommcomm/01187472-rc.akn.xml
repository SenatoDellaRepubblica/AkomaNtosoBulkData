			<an:debateSection id="d1e37" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="08:40:00"/>
					
               
					<an:i>La seduta inizia alle ore 8,40.</an:i>
					
            
				</an:narrative>
				
         
