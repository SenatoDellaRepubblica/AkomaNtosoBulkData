			<an:debateSection id="d1e37" name="InizioSeduta">
				
            
				<an:narrative title="InizioSeduta">
					
               
					<an:recordedTime time="15:25:00"/>
					
               
					<an:i> La seduta inizia alle ore 15,25.</an:i>
					
            
				</an:narrative>
				
         
