					<an:speech by="#p29527" as="#senatore">
						
                  
						<an:from refersTo="#p29527"/>
						
                  
						<an:p>
							Il presidente 
							<an:ref id="d1e96" href="#p29527">
                        PESCO
                     </an:ref>
