			</an:debateSection>
			
         
			<an:debateSection id="d1e458" name="FineSeduta">
				
            
				<an:narrative title="FineSeduta">
					
               
					<an:recordedTime time="09:40:00"/>
					
               
					<an:i>La seduta termina alle ore 9,40.</an:i>
					
            
				</an:narrative>
				
         
