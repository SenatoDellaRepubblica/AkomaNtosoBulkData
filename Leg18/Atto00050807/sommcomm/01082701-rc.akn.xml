			<an:debateSection id="sede-d1e318" name="ANTICIPAZIONE DELLA SEDUTA POMERIDIANA DI DOMANI">
				
            
				<an:heading id="h-d1e318">ANTICIPAZIONE DELLA SEDUTA POMERIDIANA DI DOMANI</an:heading>
				
            
            
				<an:debateSection id="d-d1e326" name="">
					
               
					<an:heading id="h-d1e326"/>
					
                
               
					<an:speech by="#p32582" as="#senatore">
						
                  
						<an:from refersTo="#p32582"/>
						
                  
						<an:p>
							 Il presidente 
							<an:ref id="d1e342" href="#p32582">
                        BAGNAI
                     </an:ref>
							 avverte che la seduta pomeridiana di domani, già convocata alle ore 14, è anticipata alle ore 13,30.
						</an:p>
						
                  
						<an:p> La Commissione prende atto.</an:p>
						

               
