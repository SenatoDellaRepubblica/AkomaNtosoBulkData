			<an:debateSection id="sede-d1e837" name="POSTICIPAZIONE DELLA SEDUTA POMERIDIANA DI OGGI">
				
            
				<an:heading id="h-d1e837">POSTICIPAZIONE DELLA SEDUTA POMERIDIANA DI OGGI</an:heading>
				
            
            
				<an:debateSection id="d-d1e845" name="">
					
               
					<an:heading id="h-d1e845"/>
					
                
               
					<an:narrative title="Fisionomia"> Il presidente avverte che la seduta pomeridiana di oggi, già convocata alle ore 14, avrà inizio alle ore 18.</an:narrative>
					
               
					<an:narrative title="Fisionomia"> La Commissione prende atto.</an:narrative>
					
            
				</an:debateSection>
				
            
				<an:p xmlns:si="http://www.senato.it/static/xml/def/1.0/sindisp.html" xmlns="http://docs.oasis-open.org/legaldocml/ns/akn/3.0/CSD03"/>
				
         
			</an:debateSection>
			
         
