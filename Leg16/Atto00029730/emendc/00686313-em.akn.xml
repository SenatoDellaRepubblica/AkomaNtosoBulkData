			<an:p>
				<an:docNumber>Emendamento n. 1.112 testo 3/1</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EAB" as="#tipoSenatore">Finocchiaro</an:docProponent>
