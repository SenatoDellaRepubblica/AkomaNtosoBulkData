			<an:p>
				<an:docNumber>Emendamento n. 1.84 (testo 2)</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EBB" as="#tipoSenatore">Del Pennino</an:docProponent>
