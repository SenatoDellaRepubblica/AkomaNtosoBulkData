		<an:amendmentBody>
			<an:amendmentContent>
				<an:p>
					<an:i>Sopprimere i commi 1-</an:i>
					bis 
					<an:i>e 1-</an:i>
					ter.
				</an:p>
				<an:p align="justify">
					        
					<an:i>Conseguentemente, all'articolo 82:</an:i>
				</an:p>
				<an:p align="justify">
					            
					<an:i>al comma 1, capoverso:</an:i>
					 5-
					<an:i>bis</an:i>
					», 
					<an:i>primo periodo, sostituire le parole:</an:i>
					 «96 per cento» 
					<an:i>con le seguenti:</an:i>
					 88 per cento;
				</an:p>
				<an:p align="justify">
					            
					<an:i>al comma 2, secondo periodo, sostituire le parole:</an:i>
					 «97 per cento»
					<an:i>con le seguenti:</an:i>
					 «91 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>al comma 3, sostituire, ovunque ricorrano, le parole:</an:i>
					 «96 per cento»
					<an:i>con le seguenti:</an:i>
					 «88 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>al comma 4, secondo periodo, sostituire le parole:</an:i>
					 «97 per cento» 
					<an:i>con le seguenti:</an:i>
					 «91 per cento»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>al comma 11, lettera</an:i>
					 a), 
					<an:i>sostituire le parole:</an:i>
					 «0,30 per cento» 
					<an:i>con le seguenti:</an:i>
					 «0,20 per cento».
				</an:p>
				<an:p align="justify"/>
			</an:amendmentContent>
