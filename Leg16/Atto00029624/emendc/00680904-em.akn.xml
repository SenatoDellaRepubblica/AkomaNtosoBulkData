			</an:p>
			<an:p>
				<an:docNumber>Emendamento n. 1.19</an:docNumber>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EAB" as="#tipoSenatore">Peterlini</an:docProponent>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EFB" as="#tipoSenatore">Pinzger</an:docProponent>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EKB" as="#tipoSenatore">Thaler Ausserhofer</an:docProponent>
			</an:p>
			<an:p>
				<an:docProponent refersTo="#EPB" as="#tipoSenatore">Fosson</an:docProponent>
