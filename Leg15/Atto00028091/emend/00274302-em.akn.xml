			<an:amendmentContent>
				<an:p>
					<an:i>Al comma 1, apportare le seguenti modificazioni:</an:i>
				</an:p>
				<an:p align="justify">
					            
					<an:i>a) alla lettera</an:i>
					 a), 
					<an:i>numero 5), aggiungere, in fine, le seguenti parole:</an:i>
					 «, accertata dalle Regioni ai sensi dell'articolo 1 del medesimo decreto legislativo n. 99 del 2004»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>b) alla lettera</an:i>
					 c), 
					<an:i>capoverso «3-</an:i>
					bis»: 
					<an:i>sostituire le parole:</an:i>
					 «dell'attività agricola» 
					<an:i>con le seguenti:</an:i>
					 «delle attività agricole»;
				</an:p>
				<an:p align="justify">
					            
					<an:i>c) alla lettera</an:i>
					 c), 
					<an:i>capoverso «3-</an:i>
					bis»: 
					<an:i>lettera</an:i>
					 c), 
					<an:i>aggiungere, in fine, le seguenti parole:</an:i>
					 «e la silvicoltura».
				</an:p>
				<an:p align="justify"/>
